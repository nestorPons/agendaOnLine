<div id="dlgHistorial" class="dialog">
	<span class="icon-cancel fnClose"></span>
	<div class="content-popup">
		<select name="" id="ultimosServicios">
			<option value="10" selected>mostrar 10</option>
			<option value="20">mostrar 20</option>
			<option value="50">mostrar 50</option>
		</select>
<!--
implemenar busqueda por periodo de tiempo 
		input.iconClass
			
		<input id="historialInicio" type="date">
		<input id="historialFinal" type="date">

-->
		<table class="">
			<thead>
				<tr class = "encabezado">
					<th>Opc</th><th>Fecha</th><th>Hora</th><th>Agenda</th>
				</tr>
				<tr id="hisCita" class="template">
					<td><span class ="icon-search fnBuscarCita" tittle = "Ir a la cita" ></span></td>
					<td class="hisFecha"></td>
					<td class="hisHoras"></td>
					<td class="hisAgenda"></td>
				</tr>
				<tr id="hisServicios" class="template">
					<td><i class="icon-angle-right"></i></td>
					<td colspan=4 class="hisServicio"></td>
				</tr>
			</thead>
		</table>
	</div>
</div>